<!DOCTYPE html>
<html>
  <head>
    <title>Blogs</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%blogs.forEach((x) => {%>
        <div class="blog">
            <h3><%=x.username%></h3>
            <p><%=x.text%></p>
        </div>
    <%})%>
  </body>
</html>